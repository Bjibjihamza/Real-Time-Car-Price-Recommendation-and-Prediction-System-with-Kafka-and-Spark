        link_rubrique = "https://www.moteur.ma/ar/campagne/demande-vente/"+val.title_rubrique+"/"+val.token+"?utm_source=moteur-ma&utm_medium=referral&utm_campaign="+val.utm+"&utm_content=btn-header-category";
